{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "lib": [
            "esnext",
            "esnext.asynciterable",
            "dom"
        ],
        "noImplicitAny": false,
        "esModuleInterop": true,
        "jsx": "preserve",
        "allowJs": true,
        "sourceMap": true,
        "strict": true,
        "noEmit": true,
        "baseUrl": ".",
        "paths": {
            "~/*": [
                "./*"
            ],
            "@/*": [
                "./*"
            ]
        },
        "types": [
            "@nuxt/types"
        ]
    }
}
